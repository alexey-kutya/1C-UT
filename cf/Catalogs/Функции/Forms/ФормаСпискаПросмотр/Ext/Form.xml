				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ
	СправочникФункции.Ссылка,
	СправочникФункции.ПометкаУдаления,
	СправочникФункции.Владелец,
	СправочникФункции.Родитель,
	СправочникФункции.Код,
	СправочникФункции.Наименование,
	СправочникФункции.Инструкция,
	СправочникФункции.Описание,
	СправочникФункции.Ответственный,
	СправочникФункции.ПолныйКод,
	СправочникФункции.ХранилищеИнструкции,
	СправочникФункции.ХранилищеОписания,
	СправочникФункции.КонечнаяФункция,
	СправочникФункции.ВзаимодействиеСПользователем,
	СправочникФункции.Предопределенный,
	СправочникФункции.ИмяПредопределенныхДанных,
	ВЫБОР
		КОГДА СправочникФункции.КонечнаяФункция
				И НЕ СправочникФункции.ПометкаУдаления
			ТОГДА 3
		КОГДА СправочникФункции.КонечнаяФункция
				И СправочникФункции.ПометкаУдаления
			ТОГДА 4
		КОГДА НЕ СправочникФункции.КонечнаяФункция
				И НЕ СправочникФункции.ПометкаУдаления
			ТОГДА 0
		ИНАЧЕ 1
	КОНЕЦ КАК ИндексКартинки,
	СправочникФункции.Аббревиатура
ИЗ
	Справочник.Функции КАК СправочникФункции</QueryText>
